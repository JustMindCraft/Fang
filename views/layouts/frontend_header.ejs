  <link rel="icon" type="image/png" sizes="32x32" href="https://res.cloudinary.com/da7efhqvt/image/upload/v1541724219/zhengjue/icon_logos/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="96x96" href="https://res.cloudinary.com/da7efhqvt/image/upload/v1541724219/zhengjue/icon_logos/favicon-96x96.png">
  <link rel="icon" type="image/png" sizes="16x16" href="https://res.cloudinary.com/da7efhqvt/image/upload/v1541724218/zhengjue/icon_logos/favicon-16x16.png">
  <link rel="apple-touch-icon-precomposed" href="https://res.cloudinary.com/da7efhqvt/image/upload/v1541724213/zhengjue/icon_logos/apple-icon-72x72.png">

